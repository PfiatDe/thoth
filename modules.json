    {
        "name": "get-grep-app",
        "riskLevel": "1",
        "syntax": "REPLACEME/scripts/get-grep-app '<companyName>' > <outputFile> 2>&1"
    },
    {
        "name": "get-ips-from-asn",
        "riskLevel": "1",
        "syntax": "whois -h whois.radb.net -- '-i origin <asNumber>' | grep -E '([0-9.]+){4}/[0-9]+' -C 1 > <outputFile> 2>&1"
    },
    {
        "name": "get-mails",
        "riskLevel": "2",
        "syntax": "REPLACEME/scripts/get-mails <domain> > <outputFile> 2>&1"
    },
    {
        "name": "grep-through-commits",
        "riskLevel": "1",
        "syntax": "REPLACEME/scripts/grep-through-commits <githubRepository> > <outputFile> 2>&1"
    },
    {
        "name": "gitleaks",
        "riskLevel": "1",
        "syntax": "gitleaks detect --source <githubRepositoryLocal> -v > <outputFile> 2>&1"
    },
    {
        "name": "hakrawler",
        "riskLevel": "3",
        "syntax": "echo \"https://www.<domain>\" | hakrawler -insecure > <outputFile> 2>&1"
    },
    {
        "name": "hakrawler-domain-file",
        "riskLevel": "4",                                                                                           
        "syntax": "cat <domainFile> | awk -F ' ' '{print \"https://\"$1}' | hakrawler -insecure > <outputFile> 2>&1"
    },  
    {
        "name": "hakrawler-ip-range",
        "riskLevel": "4",
        "syntax": "REPLACEME/scripts/hakrawler-ip-range <ipRange> > <outputFile> 2>&1"
    },
    {
        "name": "letItGo",
        "riskLevel": "1",
        "syntax": "letItGo <domain> > <outputFile> 2>&1"
    },
    {
        "name": "massdns",
        "riskLevel": "4",
        "syntax": "cat REPLACEME/wordlists/subdomains-top1million-110000.txt | awk -F \" \" '{print $1\".<domain>\"}' | massdns -r REPLACEME/wordlists/resolvers.txt -t A -o S -w <outputFile> > /dev/null 2>&1"
    },
    {
        "name": "nmap-reverse-lookup-ranges",
        "riskLevel": "2",
        "syntax": "nmap -sL <ipRange> | awk -F 'Nmap scan report for ' '{print $2}' | grep '(' > <outputFile> 2>&1"
    },
    {
        "name": "north-scraper",
        "riskLevel": "3",
        "syntax": "REPLACEME/scripts/north-scraper \"<companyName>\" 10 > <outputFile> 2>&1"
    },
    {
        "name": "scanrepo",
        "riskLevel": "1",
        "syntax": "cd <githubRepositoryLocal> ; git log -p | scanrepo > <outputFile> 2>&1"
    },
    {
        "name": "subfinder",
        "riskLevel": "2",
        "syntax": "subfinder --silent -d <domain> > <outputFile> 2>&1"
    },
    {
        "name": "spk",
        "riskLevel": "2",
        "syntax": "spk -silent -s '<companyName>' > <outputFile> 2>&1"
    },
    {
        "name": "spoofy",
        "riskLevel": "2",
        "syntax": "spoofy -d <domain> -o stdout > <outputFile> 2>&1"
    },
    {
        "name": "tld-discovery",
        "riskLevel": "2",
        "syntax": "REPLACEME/scripts/tld-discovery <domain> > <outputFile> 2>&1"
    },
    {
        "name": "trufflehog",
        "riskLevel": "1",
        "syntax": "trufflehog git <githubRepository> --no-update > <outputFile> 2>&1"
