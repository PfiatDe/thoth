[
    {
        "name": "amass-intel-domain",
        "riskLevel": "2",
        "syntax": "amass intel -d <domain> -whois > <outputFile> 2>&1"
    },
    {
        "name": "amass-osint",
        "riskLevel": "2",
        "syntax": "amass enum -passive -d <domain> > <outputFile> 2>&1"
    },
    {
        "name": "csprecon",
        "riskLevel": "1",
        "syntax": "csprecon -silent -u https://<domain> > <outputFile> 2>&1"
    },
    {
        "name": "csprecon-domain-file",
        "riskLevel": "2",
        "syntax": "REPLACEME/scripts/csprecon-domains <domainFile> > <outputFile> 2>&1"
    },
    {
        "name": "dork-linkedIn-employees",
